{
  "name": "friendfeed download blocker",
  "version": "0.1",
  "manifest_version": 2,
  "description": "friendfeed download blocker",
  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_icon": "ff.ico"
  },
   "permissions": ["webRequest", "webRequestBlocking",
                  "*://*.friendfeed-media.com/*",
                  "*://*.friendfeed-media.com/*"]
}