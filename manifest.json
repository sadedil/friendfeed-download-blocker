{
    "background": {
        "page": "background.html"
    }, 
    "description": "FriendFeed Download Blocker", 
    "icons": {
        "128": "icon-128.png", 
        "16": "icon-16.png", 
        "48": "icon-48.png"
    }, 
    "manifest_version": 2, 
    "name": "FriendFeed Download Blocker", 
    "page_action": {
        "default_icon": {
            "19": "icon-19.png"
        }, 
        "default_popup": "popup.html", 
        "default_title": "FriendFeed Download Blocker"
    }, 
    "permissions": [
		"tabs",
        "storage", 
        "webRequest", 
        "webRequestBlocking", 
        "*://*.friendfeed-media.com/*", 
        "*://*.friendfeed-media.com/*"
    ], 
    "version": "0.95",
	"web_accessible_resources": ["common.js"]
}
